{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": ["src"],
        "exclude": [".vscode", "public", "data\\", "dist", "doc",
         "node_modules", "test", "*.bak"],
        "includePattern": ".+\\.jsx?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": true
        },
        "docdash": {
            "collapse": false,
            "navLevel": 3
        }
    },
    "opts": {
        "encoding": "utf8",
        "private": false,
        "recurse": true,
        "template": "node_modules/docdash",
        "destination":"./docs/",
        "theme_opts": {
            "theme": "dark"
        }
    }
}
 
 
 